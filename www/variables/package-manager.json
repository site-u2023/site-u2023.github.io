{
  "packageManagers": {
    "opkg": {
      "ext": "ipk",
      "installCommand": "opkg install {allowUntrusted} {package}",
      "removeCommand": "opkg remove {package}",
      "updateCommand": "opkg update",
      "upgradeCommand": "opkg upgrade",
      "listInstalledCommand": "opkg list-installed | awk '{print $1}'",
      "dependsCommand": "opkg depends {package} 2>/dev/null | awk '/^[[:space:]]/ {gsub(/^[[:space:]]+/, \"\"); gsub(/ \\(.*\\)/, \"\"); print}'",
      "whatDependsCommand": "opkg whatdepends {package} 2>/dev/null | awk '/^[[:space:]].*depends on/ {print $1}'",
      "systemPackages": ["libc", "libgcc", "libubox", "libubus", "libuci", "luci-base", "luci-lib-", "luci-compat", "rpcd", "uhttpd", "ubus", "uci", "kernel"],
      "options": {
        "ignoreDeps": "--nodeps",
        "forceOverwrite": "--force-overwrite",
        "allowUntrusted": ""
      },
      "feeds": ["base", "packages", "luci", "routing", "telephony", "community"],
      "includeTargets": true,
      "includeKmods": true
    },
    "apk": {
      "ext": "apk",
      "installCommand": "apk add {allowUntrusted} {package}",
      "removeCommand": "apk del {package}",
      "updateCommand": "apk update",
      "upgradeCommand": "apk upgrade",
      "listInstalledCommand": "apk info",
      "systemPackages": ["libc", "libgcc", "busybox"],
      "options": {
        "ignoreDeps": "--force-broken-world",
        "forceOverwrite": "--force-overwrite",
        "allowUntrusted": "--allow-untrusted"
      },
      "feeds": ["base", "packages", "luci", "routing", "telephony", "community"],
      "includeTargets": true,
      "includeKmods": true
    }
  },
  "channels": {
    "release": {
      "opkg": {
        "versionThreshold": "23.05",
        "packageIndexUrl": "https://downloads.openwrt.org/releases/{version}/packages/{arch}/{feed}/Packages",
        "targetsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/packages/Packages",
        "kmodsIndexBaseUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/",
        "kmodsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/{kmod}/Packages"
      },
      "apk": {
        "versionThreshold": "25.12",
        "packageIndexUrl": "https://downloads.openwrt.org/releases/{version}/packages/{arch}/{feed}/index.json",
        "targetsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/packages/index.json",
        "kmodsIndexBaseUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/",
        "kmodsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/{kmod}/index.json"
      }
    },
    "snapshot": {
      "apk": {
        "versionThreshold": "24.10",
        "packageIndexUrl": "https://downloads.openwrt.org/snapshots/packages/{arch}/{feed}/index.json",
        "targetsIndexUrl": "https://downloads.openwrt.org/snapshots/targets/{vendor}/{subtarget}/packages/index.json",
        "kmodsIndexBaseUrl": "https://downloads.openwrt.org/snapshots/targets/{vendor}/{subtarget}/kmods/",
        "kmodsIndexUrl": "https://downloads.openwrt.org/snapshots/targets/{vendor}/{subtarget}/kmods/{kmod}/index.json"
      },
      "opkg": {
        "versionThreshold": "23.05",
        "packageIndexUrl": "https://downloads.openwrt.org/releases/{version}/packages/{arch}/{feed}/Packages",
        "targetsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/packages/Packages",
        "kmodsIndexBaseUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/",
        "kmodsIndexUrl": "https://downloads.openwrt.org/releases/{version}/targets/{vendor}/{subtarget}/kmods/{kmod}/Packages"
      }
    }
  }
}
