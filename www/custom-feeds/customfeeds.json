{
  "categories": [
    {
      "id": "gspotx2f",
      "name": "gSpotx2f Repository",
      "class": "tr-tui-feed-gspotx2f",
      "description": "Community packages from gSpotx2f",
      "template_path": "custom-feeds/gSpotx2f.sh",
      "api_base": "https://api.github.com/repos/gSpotx2f/packages-openwrt/contents/current",
      "download_base": "https://raw.githubusercontent.com/gSpotx2f/packages-openwrt/master/current",
      "packages": [
        {
          "id": "luci-app-log-viewer",
          "pattern": "luci-app-log-viewer",
          "exclude": "",
          "name": "luci-app-log-viewer",
          "requiresUpdate": true,
          "minOpenWrtVersion": "22.03",
          "description": "System log viewer for LuCI",
          "packageManager": ["opkg"],
          "dependencies": ["ucode", "ucode-mod-fs"]
        },
        {
          "id": "luci-app-cpu-status",
          "pattern": "luci-app-cpu-status",
          "exclude": "luci-app-cpu-status-mini",
          "name": "luci-app-cpu-status",
          "requiresUpdate": false,
          "minOpenWrtVersion": "21.02",
          "description": "CPU status monitor",
          "packageManager": ["opkg"],
          "dependencies": []
        },
        {
          "id": "luci-app-cpu-perf",
          "pattern": "luci-app-cpu-perf",
          "exclude": "",
          "name": "luci-app-cpu-perf",
          "requiresUpdate": true,
          "minOpenWrtVersion": "22.03",
          "description": "CPU performance monitor",
          "packageManager": ["opkg"],
          "dependencies": ["ucode", "ucode-mod-fs"]
        },
        {
          "id": "luci-app-temp-status",
          "pattern": "luci-app-temp-status",
          "exclude": "",
          "name": "luci-app-temp-status",
          "requiresUpdate": true,
          "minOpenWrtVersion": "22.03",
          "description": "System temperature monitor",
          "packageManager": ["opkg"],
          "dependencies": ["ucode", "ucode-mod-fs"]
        },
        {
          "id": "luci-app-disks-info",
          "pattern": "luci-app-disks-info",
          "exclude": "",
          "name": "luci-app-disks-info",
          "requiresUpdate": true,
          "minOpenWrtVersion": "21.02",
          "description": "Disk information display",
          "packageManager": ["opkg"],
          "dependencies": ["fdisk", "smartmontools", "smartmontools-drivedb"]
        },
        {
          "id": "internet-detector",
          "pattern": "internet-detector_",
          "exclude": "",
          "name": "internet-detector",
          "requiresUpdate": true,
          "minOpenWrtVersion": "21.02",
          "description": "Internet connection monitoring daemon",
          "enable_service": "internet-detector",
          "hidden": true,
          "packageManager": ["opkg"],
          "dependencies": ["lua", "luaposix", "libuci-lua"]
        },
        {
          "id": "luci-app-internet-detector",
          "pattern": "luci-app-internet-detector",
          "exclude": "",
          "name": "luci-app-internet-detector",
          "requiresUpdate": false,
          "minOpenWrtVersion": "21.02",
          "description": "LuCI interface for Internet Detector",
          "packageManager": ["opkg"],
          "dependencies": ["internet-detector"]
        },
        {
          "id": "internet-detector-mod-modem",
          "pattern": "internet-detector-mod-modem",
          "exclude": "",
          "name": "internet-detector-mod-modem",
          "requiresUpdate": true,
          "minOpenWrtVersion": "21.02",
          "description": "Modem restart module",
          "restart_service": "internet-detector",
          "packageManager": ["opkg"],
          "dependencies": ["internet-detector", "modemmanager"]
        },
        {
          "id": "internet-detector-mod-email",
          "pattern": "internet-detector-mod-email",
          "exclude": "",
          "name": "internet-detector-mod-email",
          "requiresUpdate": true,
          "minOpenWrtVersion": "21.02",
          "description": "Email notification module",
          "restart_service": "internet-detector",
          "packageManager": ["opkg"],
          "dependencies": ["internet-detector", "mailsend"]
        }
      ]
    },
    {
      "id": "jerrykuku",
      "name": "jerrykuku Repository",
      "class": "tr-tui-feed-jerrykuku",
      "description": "Argon theme and related packages from jerrykuku",
      "template_path": "custom-feeds/jerrykuku.sh",
      "api_base": "https://api.github.com/repos/jerrykuku/luci-theme-argon/releases/latest",
      "download_base": "https://github.com/jerrykuku/luci-theme-argon/releases/download",
      "packages": [
        {
          "id": "luci-theme-argon",
          "pattern": "luci-theme-argon",
          "exclude": "",
          "name": "luci-theme-argon",
          "requiresUpdate": true,
          "minOpenWrtVersion": "18.06",
          "description": "A brand new Argon theme for LuCI",
          "packageManager": ["opkg", "apk"],
          "dependencies": ["luci-compat", "luci-lib-ipkg"]
        }
      ]
    }
  ]
}
