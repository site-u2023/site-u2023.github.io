Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\sshps]
@="URL:PowerShell SSH Protocol"
"URL Protocol"=""

[HKEY_CLASSES_ROOT\sshps\shell\open\command]
@="cmd.exe /C \"where pwsh >nul 2>&1 && pwsh -NoExit -Command \\\"& {ssh root@((New-Object System.Uri '%1').Host)}\\\" || powershell -NoExit -Command \\\"& {ssh root@((New-Object System.Uri '%1').Host)}\\\"\""
