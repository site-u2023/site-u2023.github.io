Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\sshps]
@="URL:PowerShell SSH Protocol"
"URL Protocol"=""

[HKEY_CLASSES_ROOT\sshps\DefaultIcon]
@="%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"

[HKEY_CLASSES_ROOT\sshps\shell]

[HKEY_CLASSES_ROOT\sshps\shell\open]

[HKEY_CLASSES_ROOT\sshps\shell\open\command]
@="\"%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe\" -NoExit -Command \"ssh root@((New-Object System.Uri '%1').Host)\""
